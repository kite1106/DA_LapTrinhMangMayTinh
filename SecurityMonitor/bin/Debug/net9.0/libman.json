{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "provider": "unpkg",
      "destination": "wwwroot/lib/microsoft/signalr",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    },
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.js",
        "css/bootstrap.min.css",
        "css/bootstrap.css"
      ]
    },
    {
      "library": "datatables.net@2.1.1",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "js/jquery.dataTables.min.js"
      ]
    },
    {
      "library": "datatables.net-bs5@1.13.6",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "js/dataTables.bootstrap5.min.js",
        "css/dataTables.bootstrap5.min.css"
      ]
    },
    {
      "library": "toastr.js@latest",
      "destination": "wwwroot/lib/toastr",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "Chart.js@4.3.3",
      "destination": "wwwroot/lib/chartjs",
      "files": [
        "chart.umd.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [
        "jquery.validate.min.js",
        "additional-methods.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    }
  ]
}